{
  "president": {
    "description": "Repesents a single president in the system",
    "fields": [
      {
        "name": "id",
        "type": "string"
      },
      {
        "name": "name",
        "type": "string"
      },
      {
        "name": "ethnicity",
        "type": "string"
      },
      {
        "name": "dateOfBirth",
        "type": "date"
      },
      {
        "name": "yearsInOffice",
        "type": "Integer"
      },
      {
        "name": "dateStarted",
        "type": "date"
      },
      {
        "name": "dateEnded",
        "type": "date"
      },
      {
        "name": "politicalParty",
        "type": "string",
        "name": "isLiving",
        "type": "boolean"
      }
    ],
    "president_form": {
      "fields": [
        {
          "name": "name",
          "type": "string",
          "required": true
        },
        {
          "name": "ethnicity",
          "type": "string",
          "required": false
        },
        {
          "name": "dateOfBirth",
          "type": "date",
          "required": false
        },
        {
          "name": "yearsInOffice",
          "type": "integer",
          "required": true
        },
        {
          "name": "dateStarted",
          "type": "date",
          "required": true
        },
        {
          "name": "dateEnded",
          "type": "date",
          "required": true
        },
        {
          "name": "isLiving",
          "type": "boolean",
          "required": true
        }
      ]
    },
    "operations": [
      {
        "method": "GET",
        "description": "Returns a collection of all the presidents",
        "path": "/presidents",
        "responses": {
          "200": {
            "type": "[president]"
          },
          "401": {
            "type": "unit"
          },
          "404": {
            "type": "unit"
          }
        }
      },
      {
        "method": "GET",
        "description": "Returns a specific president",
        "path": "/presidents/:id",
        "responses": {
          "200": {
            "type": "president"
          },
          "401": {
            "type": "unit"
          },
          "404": {
            "type": "unit"
          }
        }
      },
      {
        "method": "POST",
        "description": "Creates a new president",
        "body": {
          "type": "president_form"
        },
        "path": "/presidents",
        "responses": {
          "201": {
            "type": "president"
          },
          "401": {
            "type": "unit"
          },
          "422": {
            "type": "error"
          }
        }
      }
    ]
  }
}
